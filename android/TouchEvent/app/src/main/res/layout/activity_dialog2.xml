<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rhino.study.touchevent.Dialog2Activity">

    <Button
        android:id="@+id/btn_items"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="Items"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/btn_sigleItems"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="singlechoiceitems"
        tools:layout_editor_absoluteX="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_items" />

    <Button
        android:id="@+id/btn_multiItmes"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="multichoiceitems"
        tools:layout_editor_absoluteX="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_sigleItems" />


</android.support.constraint.ConstraintLayout>
